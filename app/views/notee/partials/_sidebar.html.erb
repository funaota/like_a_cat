<div class="notee_sidebar">
  <h3>Category</h3>
  <div class="notee_sidebar_content">
    <ul class="notee_sidebar_ul">
      <% notee_categories.each do |key, category_array| %>
          <a href="/categories/<%= category_array[1].slug %>">
            <li><%= category_array[1].name %> (<%= category_array[0] %>)</li>
          </a>
      <% end %>
    </ul>
  </div>
  <h3>Archive</h3>
  <div class="notee_sidebar_content">
    <% notee_archives.each do |key, count| %>
        <ul class="notee_sidebar_ul">
          <a href="/archives/<%= key %>">
            <li><%= key %> (<%= count %>)</li>
          </a>
        </ul>
    <% end %>
  </div>

  <a
    class="twitter-timeline"
    href="https://twitter.com/dev_funaota"
    height="400">
    Tweets by dev_funaota</a>
  <script async src="//platform.twitter.com/widgets.js" charset="utf-8"></script>

  <div id="fb-root"></div>
  <script>(function(d, s, id) {
    var js, fjs = d.getElementsByTagName(s)[0];
    if (d.getElementById(id)) return;
    js = d.createElement(s); js.id = id;
    js.src = "//connect.facebook.net/ja_JP/sdk.js#xfbml=1&version=v2.8&appId=230256697179534";
    fjs.parentNode.insertBefore(js, fjs);
  }(document, 'script', 'facebook-jssdk'));</script>
  <div
    class="fb-page"
    data-href="https://www.facebook.com/likeacatoooooo/"
    data-tabs="timeline"
    data-small-header="false"
    data-adapt-container-width="true"
    data-hide-cover="false"
    data-show-facepile="true">
    <blockquote
      cite="https://www.facebook.com/likeacatoooooo/"
      class="fb-xfbml-parse-ignore">
      <a href="https://www.facebook.com/likeacatoooooo/">
        Like_a_cat
      </a>
    </blockquote>
  </div>
</div>
